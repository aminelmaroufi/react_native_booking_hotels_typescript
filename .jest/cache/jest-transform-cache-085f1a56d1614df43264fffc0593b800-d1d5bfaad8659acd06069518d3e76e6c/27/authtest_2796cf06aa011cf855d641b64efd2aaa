"use strict";

var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");
var _actionTypes = _interopRequireDefault(require("../../../utils/actionTypes"));
var _ = require("..");
var _user = require("../../../models/user");
describe('Test auth actions', function () {
  it('should dispatch checkUser action', function () {
    var expectedAction = {
      type: _actionTypes.default.CHECK_USER_REQUEST
    };
    expect((0, _.checkUser)()).toEqual(expectedAction);
  });
  it('should dispatch loginRequest action with the correct params', function () {
    var email = 'test@test.com';
    var password = 'Azerty!@#@@';
    var navigation = jest.fn();
    var expectedAction = {
      type: _actionTypes.default.LOGIN_REQUEST,
      email: email,
      password: password,
      navigation: navigation
    };
    expect((0, _.loginRequest)(email, password, navigation)).toEqual(expectedAction);
  });
  it('should dispatch saveAccount action with the correct params', function () {
    var account = _user.emptyUser;
    var navigation = jest.fn();
    var expectedAction = {
      type: _actionTypes.default.SAVE_ACCOUNT_REQUEST,
      account: account,
      navigation: navigation
    };
    expect((0, _.saveAccount)(account, navigation)).toEqual(expectedAction);
  });
});
