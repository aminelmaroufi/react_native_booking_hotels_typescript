exports.default = extractPolyPoints;
function extractPolyPoints(points) {
  var polyPoints = Array.isArray(points) ? points.join(',') : points;
  return polyPoints.replace(/[^eE]-/, ' -').split(/(?:\s+|\s*,\s*)/g).join(' ');
}
